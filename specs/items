start : (NEWLINE* item NEWLINE*)+

item : form WS* "::" WS* type

form : letter+

type : category
     | "(" WS* type WS* arrow WS* type WS* ")"
     | "(" WS* type WS* pipe WS* type WS* ")"
     | type WS* ":" WS* (property "," WS+)* property 

arrow : "->"

pipe : "|>"

category : "S"
         | "N"

property : word reference
         | word reference reference
         
reference : word
          | word ("." (word | number))+

number : INT

word : letter (letter | WS)+ letter

%import common.LETTER
%import common.INT
%import common.WS
%import common.NEWLINE
