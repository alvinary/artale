
cat: category (t : tile) => atlas image (t, cat.x, cat.y)

reaches(m: map element, o: map element),
r: relation(m, o) => 
has part (m, o)


parte uniforme (m, o), desentona (m, o) => False
parte distinta (m, o), entona (m, o) => False

mersa (m), fancy (o) => desentona (m, o)
playero (m), frio (n) => desentona (m, o)

simple (m), parte (m, o), contrasta (m, o) => False

desentona (m, o) => desentona(o, m)


